{% extends 'base.html.twig' %}

{% block title %}Hello BlogController!{% endblock %}

{% block body %}
<form method="POST" action="/sendData" enctype="multipart/form-data">
  Name From :<br>
  <input type="text" name="name_from"><br>
  From :<br>
  <input type="email" name="mail_from"><br>
  Name To :<br>
  <input type="text" name="name_to"><br>
  To :<br>
  <input type="email" name="mail_to"><br>

  Files :<br>
  <input type="file" name="files[]" multiple>

  <input type="submit" value="Envoyer" />
</form>
{% endblock %}
